type: egress-rule
name: myegress
namespace: default
spec:
  destination:
    service: "*.google.com"
  ports:
  - port: 80
    protocol: http
  - port: 443
    protocol: https
  - port: 8080
    protocol: http2
---
type: egress-rule
name: myegress2
namespace: default
spec:
  destination:
    service: "*.yahoo.com"
  ports:
  - port: 443
    protocol: https
use_egress_proxy: true
---
